
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
   id        Int      @id @default(autoincrement())
  clerkId    String  @unique
  photo      String  
  username  String   @unique @db.VarChar(255)
  firstName String   @db.VarChar(255)
  lastName  String   @db.VarChar(255)
  email      String  @unique
   @@index([id])
}