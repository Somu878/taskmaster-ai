generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  clerkId    String  @unique @id 
  username   String   @unique
  photo      String  
  firstname String
  lastname String
  password   String
  email      String  @unique
   @@index([clerkId])
}